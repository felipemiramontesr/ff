
<div class="node_tarjeta_de_presentacion_teaser">
  
  <div class="box_1">
    <div class="fotografia">
      {{ content.field_fotografia }}
    </div> 
  </div>

  <div class="box_2">
    <div id="container">

        <div id="row-1">
          <div class="nombre">
            {{ content.field_nombre }}
          </div>
          <div class="apellidos">
            {{ content.field_apellidos }}
          </div>
        </div>
        
        <div id="row-2">
          <div class="area_profesional">
            {{ content.field_area_profesional }}
          </div>
          <div class="body">
            {{ content.body }}
          </div>  
        </div>

        <div id="row-3">
          <div class="tecnologias">
            {{ content.field_tecnologias }}
          </div>
        </div>
        
        <div id="row-4">
          <div class="correo_electronico">
            {{ content.field_correo_electronico }}
          </div>
          <div class="numero_telefonico">
            {{ content.field_numero_telefonico }}
          </div>
        </div>
        
        <div id="row-5">
          <div class="curriculum_vitae">
            {{ content.field_curriculum_vitae }}
          </div> 
        </div>

    </div>
  </div>
</div> 

